{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": "off",
    "space-before-function-paren": "off",
    "multiline-ternary": "off",
    "no-extra-boolean-cast": "off"
  }
}
